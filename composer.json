{
  "name": "joserojasrodriguez/filament-delete-guard",
  "description": "Prevent deletion of Eloquent models in Filament with automatic notifications and UI protection.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jose Antonio Rojas Rodriguez",
      "email": "joseantoniorojasrodriguez11@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Joserojasrodriguez\\FilamentDeleteGuard\\": "src/"
    }
  },
  "require": {
    "php": "^8.2",
    "filament/filament": "^5.0",
    "spatie/laravel-package-tools": "^1.16"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Joserojasrodriguez\\FilamentDeleteGuard\\FilamentDeleteGuardServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
